;; -*- mode: clojure -*-
{:rules {:blank-lines {:trim-consecutive? true
                       :max-consecutive 1
                       :insert-padding? true
                       :padding-lines 1}
         :indentation {:indents {#"^:?require$" [[:block 0]]
                                 #"^:?import$" [[:block 0]]
                                 #"^:?use$" [[:block 0]]
                                 #"^[^ \[]" [[:inner 0]]}}
         :vars {:enabled? false}
         :namespaces {:enabled? false}}}
